{
  "name": "laika-space-game",
  "version": "1.0.0",
  "description": "A space game built with TypeScript and HTML5 Canvas",
  "main": "dist/main.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "node scripts/build.js && webpack --mode production",
    "build:analyze": "ANALYZE=true node scripts/build.js && webpack --mode production",
    "build:dev": "node scripts/build.js && webpack --mode development",
    "dev": "webpack serve --mode development --open",
    "test": "jest",
    "test:e2e": "cypress open",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "node scripts/post-build.js",
    "assets": "node scripts/build.js",
    "assets:watch": "node scripts/build.js --watch"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/cypress": "^0.1.6",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.68",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cypress": "^12.17.4",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^10.3.10",
    "glslify": "^7.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.8.0",
    "rimraf": "^5.0.10",
    "sharp": "^0.32.6",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.0",
    "typescript": "^5.7.2",
    "webpack": "^5.80.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.13.0"
  }
}
